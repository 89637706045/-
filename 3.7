#include <string.h>
#include <stdlib.h>
#include <stdio.h>
#include <ctype.h>

/* NE mogy nayti ochibky */



int main(){
	
	printf("Vvedite colichestvo slov: ");
	int m = 0;
	scanf("%d", &m);
	printf("\nVvedite colichestvo chelovek: ");
	int n = 0;
	scanf("%d", &n);
	int i = 0;
	int * voin =(int *)malloc(n*sizeof(int)); 
	
	for(i = 0; i < n; ++i ){voin[i] = i + 1;//zanomerovali
	}
	
	int counter = 0;
	int nomer_vyb = 0;
	/*colichestvo vyb voin est colichestvo xodov*/
	
	int * vyb = (int *)malloc( n * sizeof(int));
	
	while(counter < n){//ibo counter s nyla
		
		i = i + n - 1;
		while(i > (n - 1)){
			i -= i - (n - 1);
		}
		
		vyb[counter] = voin[i];
		voin[i] = 0;
		
		int help = 99;
		
		
		while(help != 0){
		if(help == 0){
			break;
		}		
			
		if((i+1) == (n-1)){
			i = 0;
		}
		else{
			++i;
		}
		
		if(i>(n - 1)){
			i -=(n-1);
		
		}
		if(voin[i] == 0){
			i++;	
		}
		else{
			help = 0;
		}
		}
		
		
		++counter;
	}
	
	
	for(i = (n - 3); i < n; ++i){
		printf("%d\n", vyb[i]);
	}
	
	free(voin);
	free(vyb);
	return 0;
}
