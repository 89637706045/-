#include<stdio.h>

int main()
{
	int n=0, n0=0, m=0, i=0, a=0, p=0;
	puts("Vvedite nachalnoe kolishestvo chelovek");//vvod dannyix
	scanf("%d", &n);
	puts(" ");
	puts("Vvedite chislo slov v chitalke");
	scanf("%d", &m);
	int r[n];//rasschet
	int mas[n];
	n=n0;
	for(i=n; i>0; i--)
	{
		while(n0-m<0)
		{
			m=m-n0;
		}
		p=a+m;
		while(p>n0)
		{
			p=p-n0;	
		}
		mas[i]=r[p];
		
		a=m+1;
		n0--;
	}
	printf("%d", mas[n]);
	printf("%d", mas[n-1]);
	printf("%d", mas[n-2]);
	return 0;
}
