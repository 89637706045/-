#include<stdio.h>
#include<stdlib.h>
#include<stdio.h>
#include<math.h>
#include<string.h>

int main()
{
	int n = 0, max = 0;
	
	scanf("%d", &n);
	
	int*a = (int*)malloc(n*sizeof(int));
	
	for(int i = 0; i < n; ++i)
	{
		scanf("%d", &a[i]);
		if(i == 0)
		{
			max = a[i];
		}
		else if(a[i] > max)
		{
			max = a[i];
		}
	}
	
	printf("%d", max);
	
	free(a);
	return 0;
}
