#include<stdio.h>
#include<stdlib.h>
#include<stdio.h>
#include<math.h>
#include<string.h>

int main()
{
	int n = 0, max = 0, counter = 0;
	
	printf("Nachinayte vvod chisel, chtoby zavechit vvod -- vedite \"Ctrl + Z\"\n");
	
	while((scanf("%d", &n)) != EOF){
	
		printf("!OTLAD! %d\n", n);
		
		if(counter == 0){
			max = n;
			++counter;
		}
		
		if(n > max){
			max = n;
		}
		
	}
	
	printf("Naibolchee iz vvodimyx chisel: %d\n", max);
	return 0;
}
